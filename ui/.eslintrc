{
  "extends": [
    "react-app",
    "airbnb",
    "plugin:jsx-a11y/recommended",
    "prettier"
  ],
  "plugins": [
    "jsx-a11y",
    "prettier"
  ],
  "rules": {
    "max-lines": ["warn", {"max": 201, "skipComments": false}],
    "camelcase": 0,
    "no-nested-ternary": 0,
    "no-param-reassign": 0,
    "no-return-assign": 0,
    "react/jsx-filename-extension": 0,
    "react/jsx-props-no-spreading": 0,
    "react/destructuring-assignment": 0,
    "react/prop-types": 0,
    "react/no-danger": 0,
    "no-unused-vars": 1,
    "react/button-has-type": 1,
    "no-underscore-dangle": ["warn", { "allow": ["_kmq", "_kmk"] }]
  }
}
